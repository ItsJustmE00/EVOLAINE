# Configuration de production pour EVOLAINE Frontend
# Copier ce fichier en .env.production et remplir les valeurs

# API Backend URL (Render)
VITE_API_BASE_URL=https://evolaine-backend.onrender.com

# Environnement
VITE_APP_ENV=production

# Configuration i18n
VITE_I18N_DEBUG=false

# Mode d√©veloppement
VITE_DEV_MODE=false

# Analytics (optionnel)
VITE_GA_TRACKING_ID=UA-XXXXXXXXX-X
VITE_GTM_ID=GTM-XXXXXXX

# Sentry pour le monitoring d'erreurs (optionnel)
VITE_SENTRY_DSN=https://xxxxx@sentry.io/xxxxx

# Stripe ou autre payment gateway (optionnel)
VITE_STRIPE_PUBLIC_KEY=pk_live_xxxxx

# reCAPTCHA pour les formulaires (optionnel)
VITE_RECAPTCHA_SITE_KEY=xxxxx
